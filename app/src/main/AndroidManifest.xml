<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.haekalsfurniture">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon8_sliding_door_closet_100px"
        android:label="Sinsa Pramuka Furniture"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Sinsa Pramuka Furniture"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JualActivity"
            android:label="Penjualan"
            android:parentActivityName=".MainActivity"/>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity"/>

        <activity android:name=".ListBarangActivity"
        android:label="List Barang"
        android:parentActivityName=".MainActivity"/>
    </application>

</manifest>